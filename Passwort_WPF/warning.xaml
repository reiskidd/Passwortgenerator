        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Passwort_WPF"
        mc:Ignorable="d"
        Title="Warning" Height="200" Width="250">
    <Grid>
        <Label x:Name="label" Content="30 Sekunden bis das Passwort&#xD;&#xA;aus der Zwischenablage &#xD;&#xA;entfernt wird." HorizontalAlignment="Left" Height="63" Margin="10,24,0,0" VerticalAlignment="Top" Width="222" FontFamily="Arial" FontSize="15"/>
        <Button x:Name="button_warning" Content="OK" HorizontalAlignment="Left" Height="36" Margin="73,108,0,0" VerticalAlignment="Top" Width="95" FontFamily="Arial" FontSize="15" Click="Button_warning_Click"/>
        

    </Grid>
</Window>
